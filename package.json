{
  "name": "hyperblog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch mock/server.ts",
    "dev:publisher": "wrangler dev --config publisher/wrangler.toml",
    "test:publisher": "tsx publisher/scripts/smoke.ts",
    "validate:content": "tsx scripts/validate-content.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "vite build && tsx scripts/postbuild.ts",
    "preview": "vite build && tsx scripts/postbuild.ts && vite preview"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "gray-matter": "^4.0.3",
    "hono": "^4.9.0",
    "lucide-react": "^0.542.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "wrangler": "^4.30.0"
  }
}
